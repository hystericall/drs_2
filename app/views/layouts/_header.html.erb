<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <a class="navbar-brand" href=<%= root_path %>>
      <img alt="Brand" src="https://d2v9k5u4v94ulw.cloudfront.net/small_light(dw=200,dh=200,da=l,ds=s,cw=200,ch=200,cc=FFFFFF)/assets/images/1347527/original/75c8cddf-1b11-4322-a17a-e20fb0baf2a7.png?1488343457" class="imagesetting">
    </a>
    <%= link_to t(".dailyreport"), root_path, class: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to t(".home"), root_path %></li>
        <li><%= link_to t(".help"), help_path %></li>
        <% if logged_in? %>
          <% if current_user.admin? %>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <%= t "ultility" %> <b class="caret"></b>
              </a>
            <ul class="dropdown-menu">
              <li><%= link_to t(".create_division"), new_division_path %></li>
              <li><%= link_to t(".create_position"), new_position_path %></li>
              <li><%= link_to t(".create_user"), signup_path %></li>
            </ul>
            </li>
          <% end %>
          <li><%= link_to t(".user"), users_path %></li>
          <li><%= link_to t(".divisions"), divisions_path %></li>
          <li><%= link_to t(".positions"), positions_path %></li>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <%= t ".account" %> <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to t(".profile"), current_user %></li>
              <li><%= link_to t(".setting"), edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to t(".logout"), logout_path, method: :delete %>
              </li>
            </ul>
          </li>
        <% else %>
          <button type="button" class="btn btn-default navbar-btn">
          <%= link_to t(".login"), login_path, class:"linkfix"%></button>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
